{
    "env": {
        "commonjs": false,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
      },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
}
